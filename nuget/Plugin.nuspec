<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.8.1">
    <id>Plugin.FacebookClient</id>
    <version>1.4.0</version>
    <title>Facebook Client Plugin for Xamarin</title>
    <authors>Rendy Del Rosario</authors>
    <owners>crossgeeks</owners>
    <licenseUrl>https://raw.githubusercontent.com/CrossGeeks/FacebookClientPlugin/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/CrossGeeks/FacebookClientPlugin</projectUrl>
    <iconUrl>https://github.com/CrossGeeks/FacebookClientPlugin/blob/master/art/icon.png?raw=true</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Facebook login, sharing, facebook graph requests and permissions handling across Xamarin.iOS and Xamarin.Android</description>
    <summary>Facebook login, sharing, facebook graph requests and permissions handling across Xamarin.iOS and Xamarin.Android</summary>
    <releaseNotes>Use of sharing dialogs instead of sharing api because of deprecation of publish_actions permission</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>iOS,Android,facebook,login,share,xamarin,plugins</tags>
    <dependencies>
        <group targetFramework="netstandard1.0">
          <dependency id="NETStandard.Library" version="1.6.1" />
        </group>
        <group targetFramework="MonoAndroid81">
            <dependency id="Xamarin.Facebook.Android" version="4.33.0" />
        </group>
        <group targetFramework="Xamarin.iOS10">
            <dependency id="Xamarin.Facebook.iOS" version="4.33.0" />
        </group>
    </dependencies>
  </metadata>
  <files>
        <!--NetStandard/PCL-->
        <file src="..\src\Plugin.FacebookClient\bin\Release\netstandard1.0\Plugin.FacebookClient.*" target="lib\netstandard1.0" />
        <file src="..\src\Plugin.FacebookClient.Android\bin\Release\Plugin.FacebookClient.*" target="lib\MonoAndroid81" />
        <file src="..\src\Plugin.FacebookClient.iOS\bin\iPhone\Release\Plugin.FacebookClient.*" target="lib\Xamarin.iOS10" />
  </files>
</package>

